# slave build pipline
pipeline {
    agent any

    stages {
        stage('GIT HUB') {
            agent{
                label'slave'
            }
            steps {
                git 'https://github.com/ravi2krishna/JavaCalculator.git'
            }
        }
    }
}

# maven failed pipeline

pipeline {
    agent any
    tool{
       maven "m3" 
    }

    stages {
        stage('build') {
            agent{
                label'slave'
            }
            steps {
                sh "mvn -Dmaven.test.failure.ignore=true clean package"
            }
        }
    }
}
